<?xml version="1.0"?>
<bindings id="wf2-bindings"
          xmlns="http://www.mozilla.org/xbl"
          xmlns:html="http://www.w3.org/1999/xhtml"
          xmlns:xul="http://www.mozilla.org/keymaster/gatekeeper/there.is.only.xul"
          xmlns:xbl="http://www.mozilla.org/xbl">

<binding id="form">
 <implementation>
  <property name="accept"/>
  <property name="data"/>
  <property name="replace"/>
  <property name="templateElements" readonly="true"/>
  <method name="checkValidity"/>
  <method name="dispatchFormChange"/>
  <method name="dispatchFormInput"/>
  <method name="resetFromData"/>
  <method name="submit"/>
 </implementation>
</binding>

<binding id="input">
 <implementation>
  <constructor>WF2Input[this.type].constructor(this)</constructor>
  <destructor>WF2Input[this.type].destructor(this)</destructor>
  <property name="autofocus">
   <getter>return WF2Input[this.type].get_autofocus(this)</getter>
   <setter>return WF2Input[this.type].set_autofocus(this, val)</setter>
  </property>
  <property name="form" readonly="true">
   <getter>return WF2Input[this.type].get_form(this)</getter>
  </property>
  <property name="forms" readonly="true">
   <getter>return WF2Input[this.type].get_forms(this)</getter>
  </property>
  <property name="labels" readonly="true">
   <getter>return WF2Input[this.type].get_labels(this)</getter>
  </property>
  <property name="list" readonly="true">
   <getter>return WF2Input[this.type].get_list(this)</getter>
  </property>
  <property name="max">
   <getter>return WF2Input[this.type].get_max(this)</getter>
  </property>
  <property name="min">
   <getter>return WF2Input[this.type].get_min(this)</getter>
  </property>
  <property name="pattern"/>
  <property name="required">
   <getter>return WF2Input[this.type].get_required(this)</getter>
   <setter>return WF2Input[this.type].set_required(this, val)</setter>
  </property>
  <property name="selectedOption" readonly="true">
   <getter>return WF2Input[this.type].get_selectedOption(this)</getter>
  </property>
  <property name="step">
   <getter>return WF2Input[this.type].get_step(this)</getter>
  </property>
  <property name="type">
   <getter>return WF2Input.get_type(this)</getter>
  </property>
  <property name="validationMessage" readonly="true">
   <getter>return WF2Input[this.type].get_validationMessage(this)</getter>
  </property>
  <property name="validity" readonly="true">
   <getter>return WF2Input[this.type].get_validity(this)</getter>
  </property>
  <property name="valueAsDate">
   <getter>return WF2Input[this.type].get_valueAsDate(this)</getter>
   <setter>return WF2Input[this.type].set_valueAsDate(this, val)</setter>
  </property>
  <property name="valueAsNumber">
   <getter>return WF2Input[this.type].get_valueAsNumber(this)</getter>
   <setter>return WF2Input[this.type].set_valueAsNumber(this, val)</setter>
  </property>
  <property name="willValidate" readonly="true">
   <getter>return WF2Input[this.type].get_willValidate(this)</getter>
  </property>
  <property name="htmlTemplate" readonly="true">
   <getter>return WF2Input[this.type].get_htmlTemplate(this)</getter>
  </property>
  <method name="checkValidity">
   <body>return WF2Input[this.type].checkValidity(this)</body>
  </method>
  <method name="dispatchChange">
   <body>return WF2Input[this.type].dispatchChange(this)</body>
  </method>
  <method name="dispatchFormChange">
   <body>return WF2Input[this.type].dispatchFormChange(this)</body>
  </method>
  <method name="setCustomValidity">
   <parameter name="error"/>
   <body>return WF2Input[this.type].setCustomValidity(this, error)</body>
  </method>
  <method name="stepDown">
   <parameter name="n"/>
   <body>return WF2Input[this.type].stepDown(this, n)</body>
  </method> 
  <method name="stepUp">
   <parameter name="n"/>
   <body>return WF2Input[this.type].stepUp(this, n)</body>
  </method> 
 </implementation>
</binding>

</bindings>
